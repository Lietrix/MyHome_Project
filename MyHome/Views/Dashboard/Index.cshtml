@model IEnumerable<MyHome.Models.Room>
@{
    ViewBag.Title = "Dashboard";
    ViewBag.WelcomeMessage = "";
}

<div>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Value)
                </td>
                <td>
                    <a href="">@Html.DisplayFor(modelItem => item.Name)</a>
                </td>
                <td>
                    <!--Html.ActionLink("Change Name", "Edit", "Rooms", new { id = item.RoomID }, "") |-->
                    <!--Html.ActionLink("Add Items", "AddItems","Rooms", new { id = item.RoomID },"") |-->
                    @Html.ActionLink("Delete", "Delete", "Rooms", new { id = item.RoomID }, "")
                </td>
            </tr>
        }

    </table>

</div>

<div>
    @Html.ActionLink("Create Room", "Create", "Rooms")
</div>
